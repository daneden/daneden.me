      "git add"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^0.15.0-1",
    "@mdx-js/tag": "^0.15.0-1",
    "gatsby": "^2.0.7",
    "gatsby-link": "^2.0.1",
    "gatsby-mdx": "0.0.6",
    "gatsby-plugin-google-analytics": "^2.0.0-rc.2",
    "gatsby-plugin-postcss": "^2.0.0-beta.2",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-styled-components": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.1-beta.10",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.2.3",
    "react": "^16.4.1",
    "react-codepen-embed": "^0.1.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-imgix": "^7.1.1",
    "react-katex": "^2.0.2",
    "remark": "^9.0.0",
    "remark-numbered-footnotes": "^1.0.5",
    "remark-react": "^4.0.3",
    "retext-smartypants": "^3.0.1",
    "slug": "^0.9.1",
    "styled-components": "^3.4.0",
    "webpack": "^4.16.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
